Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"DerivativeRules", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{"h", ",", " ", 
     RowBox[{"{", 
      RowBox[{"x", ",", "y"}], "}"}], ",", " ", "2"}], "}"}], ",", " ", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"v1", ",", "v2"}], " ", "}"}], ",", " ", "\[IndentingNewLine]", 
     
     RowBox[{
      RowBox[{
       RowBox[{"v1", "[", "x_", "]"}], " ", "=", " ", 
       RowBox[{"{", 
        RowBox[{"x", ",", " ", 
         RowBox[{"2", " ", "x"}]}], "}"}]}], ";", " ", 
      RowBox[{
       RowBox[{"v2", "[", "y_", "]"}], " ", "=", " ", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"y", "+", "2"}], ",", " ", 
         SuperscriptBox["y", "2"]}], "}"}]}], ";", " ", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"v1", "[", "x", "]"}], ".", 
       RowBox[{"v2", "[", "y", "]"}]}]}]}], "\[IndentingNewLine]", "]"}]}], 
  "]"}]], "Input",
 CellChangeTimes->{{3.934791062022417*^9, 3.934791091637024*^9}, {
